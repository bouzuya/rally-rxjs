    "path-to-regexp": "^1.2.1",
    "promised-state": "^1.0.3",
    "rxjs": "^5.0.0-beta.2",
    "vdom-parser": "^1.2.1",
    "vdom-to-html": "^2.2.0",
    "virtual-dom": "^2.1.1"
  },
  "devDependencies": {
    "babel-cli": "^6.3.17",
    "babel-preset-es2015": "^6.3.13",
    "browserify": "^13.0.0",
    "intelli-espower-loader": "^1.0.1",
    "mocha": "^2.3.4",
    "power-assert": "^1.2.0",
    "typescript": "^1.7.5",
